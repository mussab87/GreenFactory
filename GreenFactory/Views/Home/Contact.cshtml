@{
    ViewBag.title = "Contact Us";
}

<!-- START MAP -->
<div id="map">
    <iframe style="width:100%;height:335px;"
            frameborder="0"
            scrolling="no"
            marginheight="0"
            marginwidth="0"
            src="https://maps.google.it/maps?ll=41.506705,15.326647&spn=0.00687,0.016512&sll=41.506700,15.326680&amp;output=embed">
    </iframe>

    <div id="map-handler" class="container">
        <a href="#">[x] Close</a>
    </div>
</div>
<!-- END MAP -->
<!-- START PAGE META -->
<div id="page-meta" class="container" dir="rtl" lang="ar">

    <!-- BREDCRUMB -->
    <div dir="rtl" lang="ar">
       <p id="yit-breadcrumb" itemprop="breadcrumb">
        <a class="home" asp-controller="Home" asp-action="Index">الرئيسية</a> &gt;
        <a class="no-link current" asp-controller="Home" asp-action="Contact">اتصل بنا</a>
        </p>
    </div>

</div>
<!-- END PAGE META -->
<!-- START PRIMARY -->
<div id="primary" class="sidebar-right" dir="rtl" lang="ar">
    <div class="container group" dir="rtl" lang="ar">
        <div class="row" dir="rtl" lang="ar">
            <!-- START CONTENT -->
            <div id="content-page" class="span9 content group" dir="rtl" lang="ar">
                <div class="page type-page status-publish hentry group" dir="rtl" lang="ar">
                    <h3>اتصل بنا</h3>
                    <p>&nbsp;</p>
                    <form id="contact-form-contact-form" class="contact-form row" method="post" enctype="multipart/form-data" dir="rtl" lang="ar">

                        <div class="usermessagea" dir="rtl" lang="ar"></div>
                        <fieldset>
                            <ul dir="rtl" lang="ar">
                                <li class="text-field span3">
                                    <label for="name-contact-form">
                                        <span class="mainlabel">الاسم</span>
                                    </label>
                                    <div class="input-prepend" dir="rtl" lang="ar">

                                        <input type="text" name="yit_contact[name]" id="name-contact-form" class="required" value="" />
                                        <span class="add-on">
                                            <i class="icon-user"></i>
                                        </span>
                                    </div>
                                    <div class="msg-error" dir="rtl" lang="ar"></div>
                                    <div class="clear" dir="rtl" lang="ar"></div>
                                </li>

                                <li class="text-field span3">
                                    <label for="email-contact-form">
                                        <span class="mainlabel">البريد الإلكتروني</span>
                                    </label>
                                    <div class="input-prepend" dir="rtl" lang="ar">
                                        <span class="add-on">
                                            <i class="icon-envelope"></i>
                                        </span>
                                        <input type="text" name="yit_contact[email]" id="email-contact-form" class="required email-validate" value="" />
                                    </div>

                                    <div class="msg-error" dir="rtl" lang="ar"></div>
                                    <div class="clear" dir="rtl" lang="ar"></div>
                                </li>

                                <li class="text-field span3">
                                    <label for="phone-contact-form">
                                        <span class="mainlabel">رقم الهاتف</span>
                                    </label>
                                    <div class="input-prepend" dir="rtl" lang="ar">
                                        <span class="add-on">
                                            <i class="icon-phone"></i>
                                        </span>
                                        <input type="text" name="yit_contact[phone]" id="phone-contact-form" class="" value="" />
                                    </div>

                                    <div class="msg-error" dir="rtl" lang="ar"></div>
                                    <div class="clear" dir="rtl" lang="ar"></div>
                                </li>

                                <li class="textarea-field span9">
                                    <label for="message-contact-form">
                                        <span class="mainlabel">الأسئلة والاستفسارات</span>
                                    </label>
                                    <div class="input-prepend" dir="rtl" lang="ar">
                                        <span class="add-on">
                                            <i class="icon-pencil"></i>
                                        </span>
                                        <textarea name="yit_contact[message]" id="message-contact-form" rows="8" cols="30" class="required"></textarea>
                                    </div>
                                    <div class="msg-error" dir="rtl" lang="ar"></div>
                                    <div class="clear" dir="rtl" lang="ar"></div>
                                </li>

                                <li class="submit-button span9">
                                    <input type="text" name="yit_bot" id="yit_bot" />
                                    <input type="hidden" name="yit_action" value="sendmail" id="yit_action" />
                                    <input type="hidden" name="yit_referer" value="pages-contact.html" />
                                    <input type="hidden" name="id_form" value="3" />
                                    <input type="submit" name="yit_sendemail" value="ارسال" class="sendmail alignright" />
                                    <div class="clear" dir="rtl" lang="ar"></div>
                                </li>
                            </ul>
                        </fieldset>
                    </form>

                    <script type="text/javascript">
                        var messages_form_3 = {
                            name: "Insert the name",
                            email: "Insert a valid email",
                            phone: "",
                            message: "Insert a message"
                        };
                    </script>

                </div>

                <!-- START COMMENTS -->
                <!--<div id="comments"></div>-->
                <!-- END COMMENTS -->
            </div>
            <!-- END CONTENT -->
            <!-- START SIDEBAR -->
            <div id="sidebar-contact" class="span3 sidebar group" dir="rtl" lang="ar">
                <div id="contact-info-2" class="widget-first widget contact-info" dir="rtl" lang="ar">
                    <h3>كيفية الاتصال بنا</h3>
                    <div class="sidebar-nav" dir="rtl" lang="ar">
                        <ul>
                            <li>
                                <i class="icon-map-marker" style="color:#000;font-size:12px;width:12px;height:12px"></i>
                                &nbsp;
                                <span>العنوان:</span> --------------
                            </li>

                            <li>

                                <i class="icon-phone" style="color:#000;font-size:12px;width:12px;height:12px"></i>
                                &nbsp;
                                <span>رقم الهاتف</span> 555555555 966+
                            </li>

                            <li>
                                <i class="icon-print" style="color:#000;font-size:12px;width:12px;height:12px"></i>
                                &nbsp;
                                <span>الفاكس</span>  555555555 966+
                            </li>

                            <li>
                                <i class="icon-envelope" style="color:#000;font-size:12px;width:12px;height:12px"></i>
                                &nbsp;
                                <span>البريد الإلكتروني</span> Email@test.com
                            </li>
                        </ul>
                    </div>
                </div>
                <div id="text-2" class="widget-2 widget-last widget widget_text" dir="rtl" lang="ar">
                    <h3>وسائل التواصل الاجتماعي</h3>
                    <div class="textwidget" dir="rtl" lang="ar">                

                        <a href="# " class="socials-small facebook-small" title="Facebook"></a>



                        <a href="#" class="socials-small twitter-small" title="Twitter"></a>



                        <a href="#" class="socials-small linkedin-small" title="Linkedin"></a>


                    </div>
                </div>
            </div>
            <!-- END SIDEBAR -->


        </div>
    </div>
</div>
<!-- END PRIMARY -->
<!-- START CUSTOM SCRIPT -->
<script type='text/javascript'>
    var l10n_handler = { "map_close": "[x] Close", "map_open": "[x] Open" };
</script>
<!-- END CUSTOM SCRIPT -->